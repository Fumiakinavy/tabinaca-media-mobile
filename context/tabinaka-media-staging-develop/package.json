{
  "name": "gappy-tabinaka-media",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "audit": "npm audit --audit-level=high",
    "build": "next build",
    "build:cleanup": "rm -rf .next/cache .next/standalone/node_modules",
    "build:vercel": "next build",
    "check:env": "tsx scripts/check-env.ts",
    "dev": "next dev -p 2098 -H 0.0.0.0",
    "dev:tunnel": "concurrently \"npm run dev\" \"npm run tunnel\"",
    "format": "prettier --write .",
    "generate:activities": "npx ts-node --esm tools/generate-activity-sql.ts",
    "generate:activities:dry": "npx ts-node --esm tools/generate-activity-sql.ts --dry-run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "next start -p 8080",
    "test": "npm run lint",
    "tunnel": "ngrok http 2098",
    "tunnel:https": "ngrok http 2098 --host-header=localhost:2098",
    "type-check": "tsc --noEmit"
  },
  "overrides": {
    "glob": "^11.1.0",
    "js-yaml": "^3.14.2"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.946.0",
    "@hookform/resolvers": "^5.1.1",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "14.2.32",
    "@opennextjs/cloudflare": "^1.8.3",
    "@sendgrid/mail": "^8.1.5",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.10.6",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.46",
    "autoprefixer": "^10.4.16",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.0.0",
    "critters": "^0.0.23",
    "dotenv": "^17.2.2",
    "esbuild": "^0.27.1",
    "framer-motion": "^12.23.24",
    "googleapis": "^136.0.0",
    "gray-matter": "^4.0.3",
    "i18next": "^23.14.0",
    "i18next-fs-backend": "^2.6.0",
    "lucide-react": "^0.544.0",
    "next": "16.0.10",
    "next-i18next": "^15.2.0",
    "next-mdx-remote": "^5.0.0",
    "openai": "^6.4.0",
    "postcss": "^8.4.32",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.58.1",
    "react-i18next": "^13.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^10.1.0",
    "react-phone-number-input": "^3.4.12",
    "remark-gfm": "^4.0.1",
    "styled-jsx": "^5.1.7",
    "swiper": "^11.2.10",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/react-dom": "^18.2.18",
    "@types/react-i18next": "^7.8.3",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.2.32",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "globals": "^16.5.0",
